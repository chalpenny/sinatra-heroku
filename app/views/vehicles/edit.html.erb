<h1>Edit vehicle info</h1>
<form method="post" action="/vehicles/<%= @vehicle.id %>">
    <inut type="hidden" name="_method" value="patch" />
    <p>
        <label for="make;">Make:</label>
        <input type="text" name="vehicle[make]" />
    </p>
    <p>
        <label for="model;">Model:</label>
        <input type="text" name="vehicle[model]" />
    </p>
    <p>
        <label for="year;">Year:</label>
        <input type="text" name="vehicle[year]" />
    </p>
    <input type="submit" value="Update vehicle">
</form>